{
  "name": "x-hunt",
  "version": "1.0.0",
  "private": true,
  "description": "Adventure booking platform with ethical AI features",
  "keywords": [
    "adventure",
    "booking",
    "travel",
    "experiences",
    "nextjs",
    "react",
    "typescript",
    "ai"
  ],
  "homepage": "https://x-hunt.vercel.app",
  "bugs": {
    "url": "https://github.com/your-username/x-hunt/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/x-hunt.git"
  },
  "license": "MIT",
  "author": "X-hunt Team",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "postbuild": "next-sitemap",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "dev": "next dev",
    "docker:build": "docker build -t x-hunt .",
    "docker:compose": "docker-compose up -d",
    "docker:run": "docker run -p 3000:3000 x-hunt",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "react": "^18",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "jest": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}